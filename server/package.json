    "test": "NODE_ENV='test' VAULT_LOG_LEVEL='silent' mocha test/**/*.js --exit",
    "lint": "eslint . --fix --ext .js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/n6rayan/Vault.git"
  },
